{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "VS Code Extension Development",
			"type": "extensionHost",
			"request": "launch",
			"runtimeExecutable": "${execPath}",
			"args": ["--extensionDevelopmentPath=${workspaceFolder}"],
			"outFiles": ["${workspaceFolder}/out/**/*.js"],
			"preLaunchTask": "build-all"
		}
	]
} 